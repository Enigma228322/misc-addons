<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="add_loaner_bikes_tree" model="ir.ui.view">
			<field name="name">loaner.bikes.tree</field>
			<field name="model">loaner.bikes</field>
			<field name="type">tree</field>
			<field eval="8" name="priority"/>
			<field name="arch" type="xml">
				<tree>
					<field name="major_unit_id"/>
					<field name="start_time"/>
					<field name="end_time"/>
				</tree>
			</field>
		</record>
		<record id="add_loaner_bikes_form" model="ir.ui.view">
			<field name="name">loaner.bikes.form</field>
			<field name="model">loaner.bikes</field>
			<field name="type">form</field>
			<field eval="3" name="priority"/>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<field name="major_unit_id"/>
							<field name="start_time"/>
							<field name="end_time"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="add_loaner_bikes_actions" model="ir.actions.act_window">
			<field name="name">Lift</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">loaner.bikes</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="context">{}</field>
		</record>
		<record id="lift_action_tree" model="ir.actions.act_window.view">
			<field name="act_window_id" ref="add_loaner_bikes_actions"/>
			<field eval="10" name="sequence"/>
			<field name="view_mode">tree</field>
			<field name="view_id" ref="add_loaner_bikes_tree"/>
		</record>
		<record id="lift_action_form" model="ir.actions.act_window.view">
			<field name="act_window_id" ref="add_loaner_bikes_actions"/>
			<field eval="20" name="sequence"/>
			<field name="view_mode">form</field>
			<field name="view_id" ref="add_loaner_bikes_form"/>
		</record>
		<menuitem action="add_loaner_bikes_actions" id="loaner_bike_menu" name="Set Loaner Bikes" parent="settings_general_menu11" sequence="2"/>
	</data>
</odoo>
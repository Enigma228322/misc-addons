        <record id="res_partner_only_form_view" model="ir.ui.view">
            <field name="name">res.partner.product.form</field>
            <field name="model">res.partner</field>
            <field eval="900" name="priority"/>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='accounting']" position="after">
                    <page autofocus="autofocus" string="Service History">
                        <field context="{'partner_id':id}" name="customer_ride_service" nolabel="1">
                            <tree string="customer_ride_service">
                                <field name="partner_id"/>
                                <field name="major_unit_id"/>
                                <field name="state"/>
                                <field name="requested_date"/>
                                <field name="start_date"/>
                                <field attrs="{'readonly': True}" name="name"/>
                            </tree>
                        </field>
                    </page>
                    <page string="My Orders">
                        <field name="customer_product_ids">
                            <tree create="0" decoration-info="state == 'draft'" decoration-muted="state in ('done','cancel')" string="My Orders">
                                <field name="partner_id"/>
                                <field name="product_id"/>
                                <field name="state"/>
                                <field name="amount_total"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
                <xpath expr="//group" position="before">
                    <group>
                        <group col="2" name="backend">
                            <!--  <field name="skills" widget="many2many_tags" />-->
                        </group>
                    </group>
                </xpath>
            </field>
        </record>
